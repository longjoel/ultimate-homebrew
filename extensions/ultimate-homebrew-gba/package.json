{
  "name": "ultimate-homebrew-gba",
  "displayName": "ultimate-homebrew-gba",
  "description": "ultimate homebrew gba tools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "ultimate-homebrew-gba.helloWorld",
      "title": "Hello World"
    }],
    "languages": [{
      "id": "gba",
      "extensions": [".gba"]
    }],
    "configuration": {
      "fileAssociations": {
        "*.gba": "gba"
      }
    },
    "customEditors": [
      {
        "viewType": "gbaCustomEditor",
        "displayName": "GBA Custom Editor",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "*.gba"
          }
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
